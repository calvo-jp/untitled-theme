{
  "name": "@untitled-theme/icons-react",
  "type": "module",
  "version": "0.13.0",
  "license": "MIT",
  "description": "Untitled UI icons for React",
  "keywords": [
    "svg",
    "icon",
    "icons",
    "React",
    "NextJS",
    "Untitled UI"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calvo-jp/untitled-theme.git",
    "directory": "packages/icons/react"
  },
  "bugs": {
    "url": "https://github.com/calvo-jp/untitled-theme/issues"
  },
  "author": {
    "name": "John Paul Calvo",
    "email": "calvojp92@gmail.com"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "prebuild": "pnpm --filter @untitled-theme/scripts run generate:icons -f react",
    "release": "release-it"
  },
  "devDependencies": {
    "@tsconfig/vite-react": "^3.4.0",
    "@types/react": "^19.0.4",
    "react": "^19.0.0",
    "release-it": "^18.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.4.0"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0 || ^19.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "release-it": {
    "hooks": {
      "after:bump": [
        "pnpm build"
      ]
    },
    "git": {
      "commitMessage": "chore: release ${npm.name} v${version}",
      "tagName": "${npm.name}@${version}"
    },
    "github": {
      "release": false
    }
  }
}
